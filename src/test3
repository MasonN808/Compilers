
{
  int a
  a = 1
  {
    string b
    a = 2
    print(a)
  }
  string b
  b = "baa"
  if (a == 1) {
    print(b)
  }
}$

{
 int a
 boolean b
 {
 string c
 a = 5
 b = true /* no comment */
 c = "inta"
 print(c)
 }
 print(b)
 print(a)
}$


