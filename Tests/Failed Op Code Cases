{
   int a
   a = 0
   string q
   {
      a = 2
      int a
      {
         string c
         {
            a = 4
            c = "hello"
         }
         print(c)
      }
      print(a)
      q = "world"
   }
   print(a)
   print(q)
}
$


{
   int a
   boolean b
   string c
   a = 9
   b = true
   {
      print(a)
      print(b)
      b = false
      c = "hello world"
      int b
      b = 0
      {
         print(c)
         a = 1 + 2 + a
         {
            print(b)
         }
      }
      b = a
      print(b)
   }
   print(b)
}
$


